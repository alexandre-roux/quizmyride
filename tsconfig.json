{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "allowJs": true,
    "checkJs": false,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {}
  },
  "include": [
    "src",
    "vitest.setup.js",
    "vite.config.js",
    "scripts"
  ],
  "references": []
}